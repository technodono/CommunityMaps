<map proto="1.5.0">
<name>Blitz Bees</name>
<version>1.0</version>
<objective>Eliminate the enemy team!</objective>
<gamemode>br</gamemode>
<created>2024-08-12</created>
<authors>
    <author uuid="55cb168a-e28f-49e5-a853-8268093926c8"/> <!-- 0uzi -->
    <author uuid="784ed5ba-bbca-45bb-b3bf-7c2ec068ba7c"/> <!-- Celesian -->
</authors>
<contributors>
    <contributor uuid="fe3608b7-d105-4029-8800-34b3147065b6" contribution="Building help"/> <!-- rockymine -->
</contributors>
<rules>
    <rule>Fall damage is disabled!</rule>
</rules>
<teams>
    <team id="orange" color="gold" max="12">Orange</team>
    <team id="gray" color="dark gray" max="12">Gray</team>
</teams>
<broadcasts>
    <alert after="2s">Fall damage is disabled on this map! You can also double jump!</alert>
</broadcasts>
<blitz/>
<rage/>
<time>5m</time>
<kits>
    <kit id="spawn">
        <item slot="0" name="Stinger" lore="One Hit Kill" enchantment="damage all:10">gold sword</item>
        <helmet material="leather helmet" team-color="true"/>
        <chestplate material="leather chestplate" team-color="true"/>
        <leggings material="leather leggings" team-color="true"/>
        <boots material="leather boots" team-color="true"/>
        <effect duration="5s" amplifier="255">damage resistance</effect>
        <game-mode>adventure</game-mode>
        <double-jump power="3.5" recharge-before-landing="true"/>
    </kit>
</kits>
<spawns>
    <default>
        <regions>
            <cylinder base="0.5,50,0" radius="1.5" height="0"/>
        </regions>
    </default>
    <spawn team="orange" kit="spawn">
        <regions yaw="180">
            <cylinder base="0.5,11,76.5" radius="1" height="0"/>
        </regions>
    </spawn>
    <spawn team="gray" kit="spawn">
        <regions>
            <cylinder base="0.5,11,-76.5" radius="1" height="0"/>
        </regions>
    </spawn>
</spawns>
<regions>
    <apply block="never" use="never"/>
</regions>
<hunger>
    <depletion>off</depletion>
</hunger>
<disabledamage>
    <damage>fall</damage>
</disabledamage>
<toolrepair>
    <tool>gold sword</tool>
</toolrepair>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
</itemremove>
</map>