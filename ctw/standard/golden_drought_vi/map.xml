<itemkeep>
    <item>golden apple</item>
    <item>sandstone</item>
    <item>wood</item>
    <item>ladder</item>
</itemkeep>
<kill-rewards>
    <kill-reward>
        <item material="golden apple"/>
        <item amount="32" material="sandstone"/>
        <item amount="16" damage="2" material="wood"/>
    </kill-reward>
</kill-rewards>
<maxbuildheight>84</maxbuildheight>
<damage>
    <deny>
        <region id="spawn-drops"/>
    </deny>
</damage>
<item-mods>
    <rule>
        <match>
            <material>gold helmet</material>
            <material>gold chestplate</material>
            <material>gold leggings</material>
            <material>gold boots</material>
        </match>
        <modify unbreakable="true"/>
    </rule>
    <rule>
        <match>
            <material>gold pickaxe</material>
            <material>gold axe</material>
            <material>gold spade</material>
            <material>gold hoe</material>
            <material>gold sword</material>
        </match>
        <modify>
            <enchantment level="5">efficiency</enchantment>
            <enchantment level="4">unbreaking</enchantment>
        </modify>
    </rule>
</item-mods>
<crafting>
    <shaped>
        <result damage="1" material="golden apple"/>
        <shape>
            <row>GGG</row>
            <row>GAG</row>
            <row>GGG</row>
        </shape>
        <ingredient symbol="G">gold block</ingredient>
        <ingredient symbol="A">apple</ingredient>
    </shaped>
    <!-- <disable>birch wood stairs</disable>
    <disable>birch fence</disable>
    <disable>birch fence gate</disable>
    <disable>wood step</disable>
    <disable>step</disable>
    <disable>sandstone stairs</disable>
    <disable>boat</disable> -->
</crafting>
<structures>
    <structure id="skybridge-platform" air="false">
        <region>
            <cuboid min="-71,40,-18" max="-62,42,-9"/>
        </region>
    </structure>

    <dynamic structure="skybridge-platform" location="-30,40,-18"><trigger><time>30m</time></trigger></dynamic>
    <dynamic structure="skybridge-platform" location="22,40,10"><trigger><time>30m</time></trigger></dynamic>
    <dynamic structure="skybridge-platform" location="22,40,-18"><trigger><time>30m</time></trigger></dynamic>
    <dynamic structure="skybridge-platform" location="-30,40,10"><trigger><time>30m</time></trigger></dynamic>

    <dynamic structure="skybridge-platform" location="-30,48,-27"><trigger><time>45m</time></trigger></dynamic>
    <dynamic structure="skybridge-platform" location="22,48,19"><trigger><time>45m</time></trigger></dynamic>
    <dynamic structure="skybridge-platform" location="22,48,-27"><trigger><time>45m</time></trigger></dynamic>
    <dynamic structure="skybridge-platform" location="-30,48,19"><trigger><time>45m</time></trigger></dynamic>

    <dynamic structure="skybridge-platform" location="-30,56,-36"><trigger><time>60m</time></trigger></dynamic>
    <dynamic structure="skybridge-platform" location="22,56,28"><trigger><time>60m</time></trigger></dynamic>
    <dynamic structure="skybridge-platform" location="22,56,-36"><trigger><time>60m</time></trigger></dynamic>
    <dynamic structure="skybridge-platform" location="-30,56,28"><trigger><time>60m</time></trigger></dynamic>

    <dynamic structure="skybridge-platform" location="-30,64,-45"><trigger><time>75m</time></trigger></dynamic>
    <dynamic structure="skybridge-platform" location="22,64,37"><trigger><time>75m</time></trigger></dynamic>
    <dynamic structure="skybridge-platform" location="22,64,-45"><trigger><time>75m</time></trigger></dynamic>
    <dynamic structure="skybridge-platform" location="-30,64,37"><trigger><time>75m</time></trigger></dynamic>

    <dynamic structure="skybridge-platform" location="-30,72,-54"><trigger><time>90m</time></trigger></dynamic>
    <dynamic structure="skybridge-platform" location="22,72,46"><trigger><time>90m</time></trigger></dynamic>
    <dynamic structure="skybridge-platform" location="22,72,-54"><trigger><time>90m</time></trigger></dynamic>
    <dynamic structure="skybridge-platform" location="-30,72,46"><trigger><time>90m</time></trigger></dynamic>

    <dynamic structure="skybridge-platform" location="-30,80,-63"><trigger><time>105m</time></trigger></dynamic>
    <dynamic structure="skybridge-platform" location="22,80,55"><trigger><time>105m</time></trigger></dynamic>
    <dynamic structure="skybridge-platform" location="22,80,-63"><trigger><time>105m</time></trigger></dynamic>
