<map proto="1.4.2">
<name>Golden Drought VI</name>
<version>1.1.3</version>
<objective>Capture the enemy wools and return them to your victory monument.</objective>
<authors>
    <author uuid="52bcb4dd-a1a0-4d9b-94e0-49e145e318e4"/> <!-- Lich_of_Flowers -->
    <author uuid="6798905d-018d-461a-83e9-53648997d9bf"/> <!-- Penuuy -->
</authors>
<contributors>
    <author contribution="dieded" uuid="459fd919-0d58-401a-9601-6cab1eb721ab"/> <!-- Reshif -->
</contributors>
<created>2023-10-01</created>
<teams>
    <team id="blue" color="blue" max="24">Blue</team>
    <team id="red" color="dark red" max="24">Red</team>
</teams>
<broadcasts>
    <tip after="5s" every="4m">Golden tools are granted efficiency V!  Go nuts with defending.</tip>
    <!-- <tip after="25s" every="4m">Stairs cannot be crafted, along with a few blocks that would replace them.</tip> -->
    <tip after="45s">Starting at 30 minutes, the height limit will begin to increase!</tip>
    <tip after="46s">Each time this happens new skybridge islands will be spawned.</tip>
    <tip after="4m45s" count="6">Starting at 30 minutes, the height limit will begin to increase!</tip>
    <tip after="4m46s" count="6">Each time this happens, new skybridge islands will be spawned.</tip>
    <tip after="32m45s" every="4m">The height limit increases by 8 blocks every 15 minutes!</tip>
    <tip after="32m46s" every="4m">Each time this happens, new skybridge islands will be spawned.</tip>
    <alert after="30m">`c`lHeight limit increased to 44!</alert>
    <alert after="45m">`c`lHeight limit increased to 52!</alert>
    <alert after="60m">`c`lHeight limit increased to 60!</alert>
    <alert after="75m">`c`lHeight limit increased to 68!</alert>
    <alert after="90m">`c`lHeight limit increased to 76!</alert>
    <alert after="105m">`c`lHeight limit increased to 84!  It will not increase further.</alert>
    <alert after="30m2s">`cNew platform spawned!</alert>
    <alert after="45m2s">`cNew platform spawned!</alert>
    <alert after="60m2s">`cNew platform spawned!</alert>
    <alert after="75m2s">`cNew platform spawned!</alert>
    <alert after="90m2s">`cNew platform spawned!</alert>
    <alert after="105m2s">`cFinal platform spawned!</alert>
</broadcasts>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" unbreakable="true" material="stone sword"/>
        <item slot="1" unbreakable="true" material="bow">
            <enchantment>infinity</enchantment>
        </item>
        <!-- <item slot="28" material="arrow"/> we give them this later -->
        <item slot="2" unbreakable="true" material="iron pickaxe">
            <enchantment>efficiency</enchantment>
        </item>
        <item slot="3" amount="64" material="sandstone"/>
        <item slot="30" amount="64" material="sandstone"/>
        <item slot="4" amount="64" damage="2" material="wood"/>
        <item slot="31" amount="4" material="stick"/>
        <item slot="5" amount="32" material="ladder"/>
        <item slot="6" amount="4" material="web"/>
        <item slot="7" material="golden apple"/>
        <item slot="8" amount="64" material="golden carrot"/>
        <chestplate unbreakable="true" team-color="true" material="leather chestplate"/>
    </kit>
    <kit id="leave-spawn">
        <item slot="28" material="arrow"/>
    </kit>
    <kit id="no-bg-kit" force="true">
        <effect amplifier="-5">health boost</effect>
        <effect amplifier="255">wither</effect>
    </kit>
</kits>
<spawns>
    <default>
        <region yaw="-90">
            <cylinder base="-51.5,33,.5" radius="6" height="0"/>
        </region>
    </default>
    <spawn team="red" kit="spawn-kit">
        <region yaw="0">
            <cylinder base="0.5,55,-113.5" radius="1" height="0"/>
        </region>
    </spawn>
    <spawn team="blue" kit="spawn-kit">
        <region yaw="180">
            <cylinder base="0.5,55,114.5" radius="1" height="0"/>
        </region>
    </spawn>
</spawns>
<filters>
    <not id="blue-in-wr">
        <any>
            <team id="only-red">red</team>
            <material>chest</material>
        </any>
    </not>
    <not id="red-in-wr">
        <any>
            <team id="only-blue">blue</team>
            <material>chest</material>
        </any>
    </not>
    <not id="arrowless">
        <carrying>
            <item material="arrow"/>
        </carrying>
    </not>
    <all id="only-gold-regen">
        <material id="only-gold">gold block</material>
        <cause>world</cause>
    </all>
    <deny id="deny-void">
        <void/>
    </deny>
    <time id="10m">30m</time>
    <time id="20m">45m</time>
    <time id="30m">60m</time>
    <time id="40m">75m</time>
    <time id="50m">90m</time>
    <time id="60m">105m</time>
</filters>
<regions>
    <union id="no-bg">
        <cuboid id="red-base-bg" min="-9,58,-116" max="10,70,-97"/>
        <cuboid id="blue-base-bg" min="-9,58,98" max="10,70,117"/>
    </union>
    <union id="wool-rooms">
        <union id="blue-wool-rooms">
            <rectangle id="magenta-wr" min="20,-152" max="33,-165"/>
            <rectangle id="pink-wr" min="-32,-152" max="-19,-165"/>
        </union>
        <union id="red-wool-rooms">
            <rectangle id="green-wr" min="-32,153" max="-19,166"/>
            <rectangle id="lime-wr" min="20,153" max="33,166"/>
        </union>
    </union>
    <union id="spawn-drops">
        <cuboid id="blue-drop" min="-2,14,83" max="3,19,103"/>
        <cuboid id="red-drop" min="-2,14,-102" max="3,19,-82"/>
    </union>
    <complement id="void">
        <everywhere/>
        <rectangle id="mid" min="-32,-68" max="33,69"/>
        <union id="bases">
            <rectangle id="blue-base" min="-9,69" max="10,117"/>
            <rectangle id="red-base" min="-9,-116" max="10,-68"/>
        </union>
        <union id="lanes">
            <rectangle id="west-lane" min="-32,-165" max="-19,166"/>
            <rectangle id="east-lane" min="20,-165" max="33,166"/>
        </union>
        <union id="perches">
            <rectangle min="18,-123" max="20,-108"/>
            <rectangle min="-19,-123" max="-17,-108"/>
            <rectangle min="18,109" max="20,124"/>
            <rectangle min="-19,109" max="-17,124"/>
        </union>
    </complement>

    <above id="above-36" y="36"/>
    <above id="above-44" y="44"/>
    <above id="above-52" y="52"/>
    <above id="above-60" y="60"/>
    <above id="above-68" y="68"/>
    <above id="above-76" y="76"/>

    <apply kit="no-bg-kit" region="no-bg"/>
    <apply kit="leave-spawn" use="arrowless" region="spawn-drops"/>
    <apply enter="only-blue" use="only-blue" region="blue-base" message="You may not enter the enemy base!"/>
    <apply enter="only-red" use="only-red" region="red-base" message="You may not enter the enemy base!"/>
    <apply enter="only-blue" region="blue-wool-rooms" message="You may not enter your team's own wool rooms!"/>
    <apply enter="only-red" region="red-wool-rooms" message="You may not enter your team's own wool rooms!"/>
    <!-- Known bug: wool rooms have max height limit from the start of a match -->
    <apply block-break="only-gold" block-place="only-gold-regen" region="bases" message="You may only break gold blocks in the bases!"/>
    <apply block="blue-in-wr" use="only-blue" region="blue-wool-rooms"/>
    <apply block="red-in-wr" use="only-red" region="red-wool-rooms"/>

    <apply block="deny-void" region="void" message="You may not build in the void!"/>
    <apply block="60m" region="above-76" message="The height limit will increase at 105 minutes!"/>
    <apply block="50m" region="above-68" message="The height limit will increase at 90 minutes!"/>
    <apply block="40m" region="above-60" message="The height limit will increase at 75 minutes!"/>
    <apply block="30m" region="above-52" message="The height limit will increase at 60 minutes!"/>
    <apply block="20m" region="above-44" message="The height limit will increase at 45 minutes!"/>
    <apply block="10m" region="above-36" message="The height limit will increase at 30 minutes!"/>
</regions>
<portals>
    <portal y="40" z="3" yaw="180">
        <region><cuboid id="blue-portal" min="-1,15,111" max="2,18,112"/></region>
    </portal>
    <portal y="40" z="-3" yaw="180">
        <region><cuboid id="red-portal" min="-1,15,-111" max="2,18,-110"/></region>
    </portal>
</portals>
<renewables>
    <renewable region="bases" rate="1" grow="true" particles="true" avoid-entities="true">
        <renew-filter><material>gold block</material></renew-filter>
    </renewable>
</renewables>
<wools>
    <wool team="blue" color="pink" location="26.5,23.5,-157.5">
        <monument>
            <block>1,28,114</block>
        </monument>
    </wool>
    <wool team="blue" color="magenta" location="-25.5,23.5,-157.5">
        <monument>
            <block>-1,28,114</block>
        </monument>
    </wool>
    <wool team="red" color="lime" location="26.5,23.5,158.5">
        <monument>
            <block>1,28,-114</block>
        </monument>
    </wool>
    <wool team="red" color="green" location="-25.5,23.5,158.5">
        <monument>
            <block>-1,28,-114</block>
        </monument>
    </wool>
</wools>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
    <tool>iron pickaxe</tool>
    <tool>gold axe</tool>
    <tool>gold pickaxe</tool>
    <tool>arrow</tool>
    <tool>bucket</tool>
</toolrepair>
<itemremove>
    <item>leather chestplate</item>
    <item>golden carrot</item>
    <item>string</item>
    <item>glass bottle</item>
    <!-- <item>birch wood stairs</item>
    <item>birch fence</item>
    <item>birch fence gate</item>
    <item>wood step</item>
    <item>step</item>
    <item>sandstone stairs</item> -->
</itemremove>
