<?xml version="1.0"?>
<map proto="1.4.0">
<name>Xinx</name>
<version>1.0.1</version>
<objective>Capture the wool!</objective>
<gamemode>ctw</gamemode>
<created>2023-01-31</created>
<time result="objectives">30m</time>
<phase>development</phase>
<authors>
    <author uuid="f6f5dce8-a158-461d-b26d-dc8acfaeedd7"/> <!-- Shuliett -->
    <author uuid="38ef938a-7020-4c04-9d12-4875ed0598ca"/> <!-- Kazoha -->
</authors>
<teams>
    <team id="blue-team" color="blue" max="20" max-overfill="30">Blue</team>
    <team id="red-team" color="red" max="20" max-overfill="30">Red</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <clear/>
    <item slot="0" material="iron sword" unbreakable="true"/>
		<item slot="1" material="bow" unbreakable="true" enchantment="arrow infinite"/>
		<item slot="2" material="diamond pickaxe" unbreakable="true"/>
		<item slot="3" material="iron axe" unbreakable="true"/>
		<item slot="4" material="wood" amount="64"/>
		<item slot="8" material="golden apple"/>
    <item slot="27" material="wood" amount="64"/>
        <item slot="28" material="arrow"/>
        <item slot="30" material="iron spade" unbreakable="true"/>
        <leggings material="chainmail leggings" unbreakable="true"/>
        <effect duration="oo" amplifier="100">damage resistance</effect>
    </kit>
    <kit force="true" id="reset-resistance-kit">
        <effect duration="0" amplifier="0">damage resistance</effect>
    </kit>
    <kit id="blue-kit" parent="spawn-kit">
        <item slot="5" damage="11" material="stained glass" amount="64"/>
        <helmet material="leather helmet" color="5959CC" unbreakable="true"/>
        <chestplate material="leather chestplate" color="5959CC" unbreakable="true"/>
        <boots material="leather boots" color="5959CC" unbreakable="true"/>
    </kit>
    <kit id="red-kit" parent="spawn-kit">
        <item slot="5" damage="14" material="stained glass" amount="64"/>
        <helmet material="leather helmet" color="FF0C0C" unbreakable="true"/>
        <chestplate material="leather chestplate" color="FF0C0C" unbreakable="true"/>
        <boots material="leather boots" color="FF0C0C" unbreakable="true"/>
    </kit>
</kits>
<spawns>
    <spawn team="blue-team" kit="blue-kit" yaw="90" region="blue-spawn-point"/>
    <spawn team="red-team" kit="red-kit" yaw="-90" region="red-spawn-point"/>
    <default yaw="0" region="obs-spawn-point"/>
</spawns>
<wools craftable="false">
    <wool team="blue-team" color="light blue" monument="blue-team-light-blue-wool" location="-137.5,11.2,42.5"/>
    <wool team="blue-team" color="cyan" monument="blue-team-cyan-wool" location="-137.5,11.2,-41.5"/>
    <wool team="red-team" color="pink" monument="red-team-pink-wool" location="138.5,11.2,42.5"/>
    <wool team="red-team" color="magenta" monument="red-team-magenta-wool" location="138.5,11.2,-41.5"/>
</wools>
<filters>
    <team id="only-blue">blue-team</team>
    <team id="only-red">red-team</team>
    <any id="block-break-void-filter">
        <all>
            <any>
                <material>leaves</material>
                <material>log</material>
				        <material>wool</material>
                <material>stained clay</material>
                <material>melon block</material>
                <material>huge mushroom 2</material>
                <material>huge mushroom 1</material>
                <material>wood step</material>
                <material>wood double step</material>
				        <material>stained glass</material>
				        <material>spruce fence</material>
		            <material>stained clay</material>
				        <material>wood step</material>
                <material>soul sand</material>
                <material>wood</material>
                <material>log 2</material>
                <material>dirt</material>
            </any>
            <void/>
        </all>
        <not id="block-place-void-filter">
            <void/>
        </not>
    </any>
    <all id="reds-woolrooms-filter">
        <team>blue-team</team>
        <filter id="woolrooms-filter"/>
    </all>
    <all id="blues-woolrooms-filter">
        <team>red-team</team>
        <filter id="woolrooms-filter"/>
    </all>
    <any id="woolrooms-filter">
        <material>web</material>
        <material>wood:0</material>
        <material>glass</material>
    </any>
    <all id="only-iron-cause-world">
        <material id="only-iron">iron block</material>
        <cause>world</cause>
    </all>
    <material id="only-air">air</material>
</filters>
<regions>
    <cylinder id="blue-spawn-point" base="143.5,13,0.5" radius="1" height="0"/>
    <cylinder id="red-spawn-point" base="-142.5,13,0.5" radius="1" height="0"/>
    <cylinder id="obs-spawn-point" base="0.5,5,0.5" radius="1" height="0"/>
    <block id="blue-team-light-blue-wool">123,14,-8</block>
    <block id="blue-team-cyan-wool">123,14,8</block>
    <block id="red-team-magenta-wool">-123,14,8</block>
    <block id="red-team-pink-wool">-123,14,-8</block>
    <point id="light-blue-wool-spawn">-137.5,11.2,42.5</point>
    <point id="cyan-wool-spawn">-137.5,11.2,-41.5</point>
    <point id="pink-wool-spawn">138.5,11.2,42.5</point>
    <point id="magenta-wool-spawn">138.5,11.2,-41.5</point>
    <negative id="not-spawns">
        <union id="spawns">
            <rectangle id="red-spawn" min="-113,-20" max="-162,21"/>
            <rectangle id="blue-spawn" min="114,-20" max="163,21"/>
        </union>
    </negative>
    <union id="blues-woolrooms"> <!-- blue defends / red attacks -->
        <rectangle id="magenta-woolroom" min="119,-22" max="158,-61"/>
        <rectangle id="pink-woolroom" min="119,23" max="158,62"/>
    </union>
    <union id="reds-woolrooms"> <!-- red defends / blue attacks -->
        <rectangle id="cyan-woolroom" min="-118,-22" max="-157,-61"/>
        <rectangle id="light-blue-woolroom" min="-118,23" max="-157,62"/>
    </union>
    <negative id="not-build-area">
        <union id="build-area">
            <rectangle min="-12,-26" max="13,-39"/> <!--mid lane-->
            <rectangle min="-12,27" max="13,40"/> <!--mid lane-->
        </union>
    </negative>
    <union id="iron-regen">
        <cuboid min="130,18,-8" max="150,14,-4"/> <!-- blue right iron -->
        <cuboid min="130,18,9" max="150,14,5"/> <!-- blue left iron -->
        <cuboid min="-129,18,9" max="-149,14,5"/> <!-- red right iron -->
        <cuboid min="-129,18,-8" max="-149,14,-4"/> <!-- red left iron -->
    </union>
    <!-- applicators -->
    <apply enter="only-blue" region="blue-spawn" message="You may not enter the enemy's spawn!"/>
    <apply enter="only-red" region="red-spawn" message="You may not enter the enemy's spawn!"/>
    <apply enter="only-red" region="blues-woolrooms" message="You may not enter your own wool room!"/>
    <apply enter="only-blue" region="reds-woolrooms" message="You may not enter your own wool room!"/>
    <apply block-place="only-iron-cause-world" block-break="only-iron" region="spawns" message="You may not edit spawn!"/>
    <apply block="blues-woolrooms-filter" region="blues-woolrooms" message="You may not edit the wool room!"/>
    <apply block="reds-woolrooms-filter" region="reds-woolrooms" message="You may not edit the wool room!"/>
    <apply block-place="block-place-void-filter" block-break="block-break-void-filter" region="not-build-area" message="You may not edit the void!"/>
    <apply kit="reset-resistance-kit" region="not-spawns"/>
</regions>
<spawners>
    <spawner spawn-region="cyan-wool-spawn" player-region="cyan-woolroom" delay="3s">
        <item material="wool" amount="3" damage="9"/>
    </spawner>
    <spawner spawn-region="light-blue-wool-spawn" player-region="light-blue-woolroom" delay="3s">
        <item material="wool" amount="3" damage="3"/>
    </spawner>
    <spawner spawn-region="magenta-wool-spawn" player-region="magenta-woolroom" delay="3s">
        <item material="wool" amount="3" damage="2"/>
    </spawner>
    <spawner spawn-region="pink-wool-spawn" player-region="pink-woolroom" delay="3s">
        <item material="wool" amount="3" damage="6"/>
    </spawner>
</spawners>
<itemkeep>
    <item>iron sword</item>
    <item>bow</item>
    <item>diamond pickaxe</item>
    <item>iron axe</item>
    <item>arrow</item>
    <item>iron spade</item>
    <item>wood</item>
    <item>glass</item>
    <item>golden apple</item>
</itemkeep>
<timelock>on</timelock>
<itemremove>
    <item>stained glass</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>chainmail leggings</item>
    <item>leather boots</item>
    <item>stone</item>
	<item>string</item>
	<item>red rose</item>
	<item>long grass</item>
	<item>sand</item>
	<item>dark oak stairs</item>
	<item>spruce wood stairs</item>
	<item>quartz block</item>
	<item>prismarine shard</item>
	<item>wood step</item>
</itemremove>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>diamond pickaxe</tool>
    <tool>iron axe</tool>
    <tool>arrow</tool>
    <tool>iron spade</tool>
</toolrepair>
<item-mods>
    <rule>
        <match>
            <material>iron helmet</material>
            <material>iron chestplate</material>
            <material>iron leggings</material>
            <material>iron boots</material>
            <material>iron sword</material>
            <material>diamond pickaxe</material>
            <material>iron axe</material>
            <material>iron spade</material>
        </match>
        <modify unbreakable="true"/>
    </rule>
</item-mods>
<crafting>
    <!-- bulkcrafting -->
    <shaped override="false">
        <result material="iron helmet" amount="9"/>
        <shape>
            <row>III</row>
            <row>I.I</row>
        </shape>
        <ingredient symbol="I">iron block</ingredient>
    </shaped>
    <shaped override="false">
        <result material="iron chestplate" amount="9"/>
        <shape>
            <row>I.I</row>
            <row>III</row>
            <row>III</row>
        </shape>
        <ingredient symbol="I">iron block</ingredient>
    </shaped>
    <shaped override="false">
        <result material="iron leggings" amount="9"/>
        <shape>
            <row>III</row>
            <row>I.I</row>
            <row>I.I</row>
        </shape>
        <ingredient symbol="I">iron block</ingredient>
    </shaped>
    <shaped override="false">
        <result material="iron boots" amount="9"/>
        <shape>
            <row>I.I</row>
            <row>I.I</row>
        </shape>
        <ingredient symbol="I">iron block</ingredient>
    </shaped>
    <shaped override="false">
        <result material="bucket" amount="9"/>
        <shape>
            <row>I.I</row>
            <row>.I.</row>
        </shape>
        <ingredient symbol="I">iron block</ingredient>
    </shaped>
</crafting>
<renewables>
    <renewable rate="0.5" sound="false" region="iron-regen" renew-filter="only-iron" replace-filter="only-air" avoid-players="5"/>
</renewables>
<hunger>
    <depletion>off</depletion>
</hunger>
<maxbuildheight>33</maxbuildheight>
<kill-rewards>
	<kill-reward>
		<item material="golden apple" amount="1"/>
		<item material="wood" amount="16"/>
	</kill-reward>
</kill-rewards>
</map>
